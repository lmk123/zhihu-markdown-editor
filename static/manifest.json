{
  "name": "知乎.md",
  "description": "让你可以使用 Markdown 语法撰写你的知乎问题和答案。",
  "manifest_version": 2,
  "version": "0.0.0",
  "permissions": [
    "https://www.zhihu.com/*",
    "https://zhuanlan.zhihu.com/*",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.zhihu.com/*"],
      "css": ["content.css"],
      "js": ["js/content.js"]
    }
  ],
  "minimum_chrome_version": "41"
}
